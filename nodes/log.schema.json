{
  "name": "node-log",
  "states": {
    "idle": {},
    "создание": {},
    "удаление": {}
  },
  "context": {
    "queue": {
      "type": "array",
      "required": true,
      "default": []
    }
  },
  "reactions": {
    "reactions": {
      "Добавляет в очередь состояние_0": {
        "label": "Добавляет в очередь состояние",
        "cond": {
          "meta": "canvas",
          "actor": "0",
          "path": "/state",
          "op": "replace"
        },
        "read": ["queue"],
        "write": ["queue"],
        "src": "({ update, patch, context }) => {\n          update({ queue: [...context.queue, patch.value] })\n        }"
      }
    },
    "states": {
      "idle": ["Добавляет в очередь состояние_0"],
      "создание": ["Добавляет в очередь состояние_0"],
      "удаление": ["Добавляет в очередь состояние_0"]
    }
  }
}
